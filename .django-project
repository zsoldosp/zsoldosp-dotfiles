MANAGE_PY=/path/to/manage_py
DJANGO_CONFIG=settings
DJANGO_TEST_CONFIG=settings_test
DJANGO_TEST_COMMAND=test

function manage_py_test() {
        python $MANAGE_PY $DJANGO_TEST_COMMAND $* --settings=${DJANGO_TEST_CONFIG}
}
function manage_py() {
        python $MANAGE_PY $* --settings=${DJANGO_CONFIG}
}

